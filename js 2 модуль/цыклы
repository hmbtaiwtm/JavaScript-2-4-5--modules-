let num = 50;

while (num <= 55) {
    console.log(num);
    num++
}

//цыкл while будет работать до тех пор пока переменная numb не  дойдет до 55

do {
    console.log(num);  // тоже самое что и первое просто мы говорим что какоето действие в нутри do будет
    num++              // выполняться до тех пор пока будет ровно 55
}

while (num <55);

for (let i = 1; i < 8; i++); // часто используемый цыкл в котором мы пишем сначала for а внутри можем создать 
// новую переменную и внутри скобок создать условия при которыйх он будет работать, в данно случае
// в данном случае мы говорим что он бдует раотать до тех пор пока не юудет равен 8 и с каждым разом будет
// прибовляться на еденицу с помощью оператора ++

for (let i = 1; i < 8; i++);{ // ну а дальше помещаем внутри то что будет выполняться при таких условиях
    console.log(i)
}

 for (let i = 1; i < 10; i++);{ //оператор breake останавливает цыкл если наше значение будет ровно указаному нами
    if (i === 6) {
        console.log('Hello')    // оператор continue позводяет на пропустить заданое число при условие
    }                          // тоесть цыкл продолжиться даже если число будет ровно 6, оно просто пропустит его
}

